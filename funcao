//CALCULADORA USANDO FUNÇÃO



function calculadora(){

const valorA = parseInt(prompt ('digite o número A:'))
const  valorB = parseInt(prompt ('digite o número B:'))
resultadosoma = parseInt (valorA) + parseInt (valorB)
resultadosub = parseInt (valorA) - parseInt (valorB)
resultadomult = parseInt (valorA) * parseInt (valorB)
resultadodiv = parseInt (valorA) / parseInt (valorB)

alert('soma:'+ resultadosoma)
alert('subtração:'+ resultadosub)
alert('multiplicação:'+ resultadomult)
alert('divisão:'+ resultadodiv) 
}
calculadora()
